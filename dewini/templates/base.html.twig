    <script>$(document).ready(function() {
    var rating = parseInt($('input[name="form[note]"]').val());
    console.log("Valeur de la note récupérée:", rating);
    $('.star-rating .fa-star').removeClass('checked');
    $('.star-rating .fa-star').each(function() {
        if ($(this).data('rating') <= rating) {
            console.log("Étoile avec la note correspondante:", $(this).data('rating'));
            $(this).addClass('checked');
        }
    });
});

$('.star-rating .fa-star').on('mouseenter', function() {
    var rating = $(this).data('rating');
    $('.star-rating .fa-star').removeClass('hover');
    $(this).prevAll().addBack().addClass('hover');
});

$('.star-rating').on('mouseleave', function() {
    $('.star-rating .fa-star').removeClass('hover');
});

$('.star-rating .fa-star').on('click', function() {
    var rating = $(this).data('rating');
    $('.star-rating .fa-star').removeClass('checked');
    $(this).prevAll().addBack().addClass('checked');
    $('input[name="form[note]"]').val(rating);
});

</script>

